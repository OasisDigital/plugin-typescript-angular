      "npm:@angular/compiler@2.2.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/compiler": ".",
          "./testing": "./bundles/compiler-testing.umd.js"
        }
      },
      "npm:@angular/core@2.2.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/core": ".",
          "./testing": "./bundles/core-testing.umd.js"
        }
      },
      "npm:@angular/platform-browser-dynamic@2.2.0": {
        "meta": {
          "src/*.js": {
